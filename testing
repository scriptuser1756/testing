                if not i:FindFirstChild("bp") then
                    local m = i:GetMass()
                    bp = Instance.new("BodyPosition")
                    bp.Name = "bp"
                    bp.P = m/0.64e-5
                    bp.D = m/0.64e-3
                    bp.MaxForce = Vector3.new(m/0.64e-6,m/0.64e-6,m/0.64e-6)
                    bp.Parent = i
                else
                    bp = i:FindFirstChild("bp")
                end
                --[[local bgr = nil
