-- Function to update the canvas size based on the number of lines
local function updateCanvasSize()
    local lineCount = #textBox.Text:split("\n")  -- Split the text by lines and count the lines
    local newHeight = math.max(lineCount * textBox.TextSize, 135)  -- Increase height with each line, minimum height 135
    scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, newHeight)  -- Adjust the canvas size to fit the content
    textBox.Size = UDim2.new(1, 0, 0, newHeight)  -- Adjust TextBox height to match
end

-- Update the canvas size whenever the text changes
textBox:GetPropertyChangedSignal("Text"):Connect(updateCanvasSize)

-- Initial update to set the correct canvas size when the script starts
updateCanvasSize()

