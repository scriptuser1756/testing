executeStroke.Thickness = 3
executeStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
executeStroke.LineJoinMode = Enum.LineJoinMode.Round

local clearButton = Instance.new("TextButton")
clearButton.Size = UDim2.new(0, 150, 0, 50)
clearButton.Position = UDim2.new(0.6, 0, 0.63, 0)
clearButton.BackgroundColor3 = Color3.fromRGB(139, 0, 255) -- Purple clear button
clearButton.Text = "Clear"
clearButton.TextColor3 = Color3.fromRGB(255, 255, 255)
clearButton.Font = Enum.Font.Roboto
clearButton.TextSize = 35
clearButton.Parent = frame

local clearStroke = Instance.new("UIStroke")
clearStroke.Parent = clearButton
clearStroke.Color = Color3.fromRGB(0, 0, 0)
clearStroke.Thickness = 3
clearStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
clearStroke.LineJoinMode = Enum.LineJoinMode.Round

local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0, 150, 0, 50)
closeButton.Position = UDim2.new(0.1, -21, 0.8, 0)
closeButton.BackgroundColor3 = Color3.fromRGB(105, 105, 105)
closeButton.Text = "Close"
closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
closeButton.Font = Enum.Font.Roboto
closeButton.TextSize = 35
closeButton.Parent = frame

local closeStroke = Instance.new("UIStroke")
closeStroke.Parent = closeButton
closeStroke.Color = Color3.fromRGB(0, 0, 0)
closeStroke.Thickness = 3
closeStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
closeStroke.LineJoinMode = Enum.LineJoinMode.Round

local deleteButton = Instance.new("TextButton")
deleteButton.Size = UDim2.new(0, 150, 0, 50)
deleteButton.Position = UDim2.new(0.6, 0, 0.8, 0)
deleteButton.BackgroundColor3 = Color3.fromRGB(255, 0, 255) -- Magenta delete button
deleteButton.Text = "Delete gui"
deleteButton.TextColor3 = Color3.fromRGB(255, 255, 255)
deleteButton.Font = Enum.Font.Roboto
deleteButton.TextSize = 35
