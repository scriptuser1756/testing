Section:NewButton("c00lsploit", "включает скрипт экзекутор", function()
    local UserInputService = game:GetService("UserInputService")

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "MyExecutorGUI"
screenGui.Parent = game:GetService("CoreGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 405, 0, 300)
frame.Position = UDim2.new(0.5, -200, 0.5, -150)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BorderSizePixel = 4
frame.BorderColor3 = Color3.fromRGB(128, 0, 128) -- Purple outline
frame.Parent = screenGui

local headerFrame = Instance.new("Frame")
headerFrame.Size = UDim2.new(0.996, 0, 0, 50)
headerFrame.Position = UDim2.new(0, 1.7, 0, -12)
headerFrame.BackgroundColor3 = Color3.fromRGB(45, 0, 75) -- Dark purple
headerFrame.Parent = frame

local headerGradient = Instance.new("UIGradient")
headerGradient.Parent = headerFrame
headerGradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(18, 0, 32)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(45, 0, 75))
}

local headerText = Instance.new("TextLabel")
headerText.Size = UDim2.new(0.83, 0, 1, 0)
headerText.BackgroundTransparency = 1
headerText.Text = "c00lsploit"
headerText.Position = UDim2.new(-0.2, 0, 0, 0)
headerText.TextColor3 = Color3.fromRGB(255, 255, 255)
headerText.Font = Enum.Font.Merriweather
headerText.TextScaled = true
headerText.Parent = headerFrame

local headerStroke = Instance.new("UIStroke")
headerStroke.Parent = headerText
headerStroke.Color = Color3.fromRGB(0, 0, 0)
headerStroke.Thickness = 3
headerStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
headerStroke.LineJoinMode = Enum.LineJoinMode.Round

local dragging
local dragInput
local dragStart
local startPos

local function updateFramePosition(input)
    local delta = input.Position - dragStart
    frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

frame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = frame.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

frame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        updateFramePosition(input)
    end
end)

local scrollingFrame = Instance.new("ScrollingFrame")
scrollingFrame.Size = UDim2.new(0, 370, 0, 135)
scrollingFrame.Position = UDim2.new(0.5, -183, 0.15, 0)
scrollingFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
scrollingFrame.ScrollBarThickness = 10
scrollingFrame.ScrollBarImageColor3 = Color3.fromRGB(255, 255, 255)
scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 135)  -- Initial size to match the TextBox height
scrollingFrame.Parent = frame

local textBox = Instance.new("TextBox")
textBox.Size = UDim2.new(1, 0, 0, 135)  -- Initial size of TextBox
textBox.Position = UDim2.new(0, 0, 0, 0)
textBox.PlaceholderText = "Enter your Lua script here..."
textBox.Text = ""
textBox.ClearTextOnFocus = false
textBox.MultiLine = true
textBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
textBox.Font = Enum.Font.SourceSans
textBox.TextSize = 20
textBox.TextXAlignment = Enum.TextXAlignment.Left
textBox.TextYAlignment = Enum.TextYAlignment.Top
textBox.TextWrapped = true
textBox.Parent = scrollingFrame

-- Function to update the canvas size based on the number of lines
local function updateCanvasSize()
    local lineCount = #textBox.Text:split("\n")  -- Split the text by lines and count the lines
    local newHeight = math.max(lineCount * textBox.TextSize, 135)  -- Increase height with each line, minimum height 135
    scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, newHeight)  -- Adjust the canvas size to fit the content
    textBox.Size = UDim2.new(1, 0, 0, newHeight)  -- Adjust TextBox height to match
end

-- Update the canvas size whenever the text changes
textBox:GetPropertyChangedSignal("Text"):Connect(updateCanvasSize)

-- Initial update to set the correct canvas size when the script starts
updateCanvasSize()

local executeButton = Instance.new("TextButton")
executeButton.Size = UDim2.new(0, 150, 0, 50)
executeButton.Position = UDim2.new(0.1, -21, 0.63, 0)
executeButton.BackgroundColor3 = Color3.fromRGB(72, 61, 139) -- Dark purple button
executeButton.Text = "Execute"
executeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
executeButton.Font = Enum.Font.Roboto
executeButton.TextSize = 35
executeButton.Parent = frame

local executeStroke = Instance.new("UIStroke")
executeStroke.Parent = executeButton
